extends ../layout
block content
	form#form1.stdform(method='post', action='')
		.par.control-group
			label.control-label(for='title') Request Title
			.controls
				input#companyname.input-large(type='text', name='title', required)
		.control-group
			label.control-label(for='location') Location
			.controls
				input#contractno.input-large(type='text', name='location', required)
		p.stdformbutton
			button.btn.btn-primary Submit

